{
  "expo": {
    "owner": "quicksteps",
    "name": "Quicksteps Zeiterfassung",
    "slug": "quicksteps-timerecording-app",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "2.0.42",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#282d38",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#005cb9"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.quicksteps.quicksteps-timerecording",
      "backgroundColor": "#282d38",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos.",
        "NSLocationWhenInUseUsageDescription": "The app needs access to your location to track your records and activities in foreground",
        "NSLocationAlwaysUsageDescription": "The app needs access to your location to track your records and activities in background",
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to the camera to take photos for the user to track their records"
      },
      "buildNumber": "6"
    },
    "description": "Quicksteps",
    "android": {
      "versionCode": 2,
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.quicksteps.quicksteps_timerecording",
      "backgroundColor": "#282d38",
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_NETWORK_STATE",
        "ACCESS_MEDIA_LOCATION",
        "MANAGE_MEDIA",
        "ACCESS_WIFI_STATE",
        "CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "true",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.ACCESS_BACKGROUND_LOCATION"
      ]
    },
    "userInterfaceStyle": "dark",
    "extra": {
      "eas": {
        "projectId": "6bf15514-00a0-433b-bbef-edc68e7c5651"
      }
    }
  }
}
